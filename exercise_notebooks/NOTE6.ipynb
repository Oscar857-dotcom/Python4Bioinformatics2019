    "def percentageGC(trna):\n",
    "    trna_total=len(trna) \n",
    "    trna_GCtotal=(trna.count('G')+trna.count('C'))\n",
    "    trna_prcnt=trna_GCtotal/trna_total*100\n",
    "    return trna_prcnt\n",
    "trna_prcntg = percentageGC('AAGGGCTTAGCTTAATTAAAGTGGCTGATTTGCGTTCAGTTGATGCAGAGTGGGGTTTTGCAGTCCTTA')\n",
    "print(trna_prcntg)"
   ]
